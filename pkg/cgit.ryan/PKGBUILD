pkgrel=1
pkgdesc='Ryanns configured cgit'
arch=('any')
url='https://github.com/RyanBruno/TheRepository'
license=('GPL')
depends=('cgit')
backup=('etc/cgitrc')
source=(cgitrc)
sha256sums=('SKIP')
#validpgpkeys=('915EBB9C9889E4AEB983CD96F15F942AA25D5A6A')

package() {
    mkdir -p $pkgdir/etc
    install cgitrc $pkgdir/etc/cgitrc
}
